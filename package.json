{
  "name": "pathless",
  "version": "0.1.0",
  "description": "A lightweight, flexible routing library for Node.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./middleware": {
      "require": "./dist/middleware/index.js",
      "import": "./dist/middleware/index.mjs",
      "types": "./dist/middleware/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "http"
  ],
  "files": [
    "dist/",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://github.com/frolleks/pathless",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frolleks/pathless.git"
  },
  "author": "Frolleks",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.5",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "find-my-way": "^9.0.1"
  }
}
